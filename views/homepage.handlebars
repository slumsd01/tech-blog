<div class="post-list">
    {{#each posts as |post|}}
    <article class="post">
        <div class="post-header">
            <h3 class="post-title">{{post.title}}</h3>
            <p>Posted by {{post.user.username}} <br>
            on {{post.created_at}}</p>
        </div>
        <div class="post-body">
            <p>({{post.post_body}})</p>
        </div>
    </article>
    {{/each}}
</div>